/*for SimpleSearch and SearchProperties form control alignment */
.dijitToggleButton {
    margin: 0;
}
.fixht {
    height: 33px;
}
.col1 {
    min-width: 80px;
}
.col2 {
    padding-left: 5px;
}
.col3 {
    min-width: 45px;
}
.fixht3 {
    height: 40px;
}

.dgrid {
    height: auto;
}
.dgrid .dgrid-header {
    font-size: 10px;
}
.dgrid-row-odd {
    background: #F2F5F9;
}

.field-SiteID {
    width: 65px;
}
.field-DatasetID {
    width:75px;
}
.field-SiteName {
    width: 200px;
}
.field-LatitudeNorth {
    width: 75px;
}
.field-LongitudeWest {
    width: 75px;
}
.field-AgeOldest {
    width: 75px;
}
.field-AgeYoungest {
    width: 85px;
}

/*for grid in advanced taxa widget*/
.field-selCol {
    width: 30px;
}
.fixht2 {
    height: 220px;
}

/* site pop-up*/
.sitepop .col1 {
    min-width: 80px;
    font-weight: bold;
    color: #898A8B;
    vertical-align: top;
}
.sitepop .col2 {
    padding-left: 5px;
    padding-bottom: 5px;
    vertical-align: top;
    word-wrap: break-word;
    max-width: 165px;
    min-width: 165px;
}

.sitepop.dsToggle {
    display: inline;
    float: left;
}
.sitepop.dsToggle div {
    display: inline;
    padding: 0;
    cursor: pointer;
    font-size: .9em;
    color: #808080;
    vertical-align: top;
}
.sitepop.dsToggle .dstActive {
    font-weight: bold;
    color: #555454;
}

.mini, .mini div {
    padding: 0;
    border: 0;
 }
.mini button, button.mini {
    width: 18px;
    height: 16px;
    cursor: pointer;
    padding: 0;
    border: 0;
}
button.mini {
    float: right;
}
.mini.dsAddAll {
    height: 14px;
    background: url("../resources/images/folder_add14.png") no-repeat center;
}
.mini button.dsView {
    background: url("../resources/images/eye16.png") no-repeat center;
}
.mini button.dsAdd, .dsAdd {
    background: url("../resources/images/add16.png") no-repeat center;
}
.mini button.dsRemove {
    background: url("../resources/images/erase12.png") no-repeat center;
}
.mini button.dsSave {
    background: url("../resources/images/disk.png") no-repeat center;
}
.mini button.dsEmail {
    background: url("../resources/images/email_open.png") no-repeat center;
}
.mini .dijitTooltipContainer, .mini .dijitTooltipContents.dijitTooltipFocusNode {
    padding: 0;
}
.mini .dijitTooltipConnector {
    display: none;
}
.mini .dijitToolbar {
    border: 0;
    background-color: transparent;
    background-image: none;
    padding: 2px;
}

/* chron metadata */
.chronmeta .lbl, .stratDiagram tr.lbl {
    font-weight: bold;
    color: #898A8B;
    height: 28px;
    vertical-align: bottom;
}

/* stratigraphic diagram left pane */
.stratDiagram tr {
    vertical-align: top;
}
.stratDiagram td.c1 {
    padding-left: 5px;
}
.spacer {
    margin: 10px 0 25px 0;
}

/* site metadata */
.claro h3 {
    color: #757575;
}
.sitemeta {
    margin-left: 10px;
}
.sitemeta .col1 {
    font-weight: bold;
    color: #898A8B;
    width: 80px;
    vertical-align: top;
}
.sitemeta .col2 {
    padding-left: 10px;
    max-width: 500px;
    vertical-align: top;
}

/* publications */
p.cite {
    max-width: 700px;
    padding-left: 3em;
    text-indent: -3em;  /* hanging indent */
    margin-left: 10px;
}

/* specific widths for columns in dataset samples grid*/
.wideCols {
    height: auto;
}
.wideCols .dgrid-row {
    height: 22px;
}
.wideCols .dgrid-cell {
    width: 80px; /* force all columns to have SOME width */
    text-overflow: ellipsis;
    white-space: nowrap;
}
.wideCols .field-name {
    width: 125px;
}
.wideCols .field-group {
    width: 95px;
}
.wideCols .field-element {
    width: 70px;
}
.wideCols .field-units {
    width: 60px;
}
.wideCols .field-context {
    width: 60px;
}

