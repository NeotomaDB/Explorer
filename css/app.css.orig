* {
    outline: none !important;
}
body, html {
    height: 100%;
    margin: 0;
    padding: 0;
    overflow: hidden;
    font-family: Verdana, Arial, Helvetica, sans-serif;
}

.claro {
    font-family: Verdana,Arial,Helvetica,sans-serif;
    font-size: .75em; /*.9 is default? was .8*/
    color: #131313;
}

a:link, a:visited {
    text-decoration:none;
}
a:hover, a:active {
    text-decoration:underline;
}

#content {
    height: 100%;
}
#topPane {
    padding: 0;
}
#toolbar1 {
    background-color: #D8E4F2;
    padding: 5px 5px 5px 200px;
}
#toolbar1 #toolbar\.about {
    margin: 7px 2px 7px 2px;
}
#logo {
    position: absolute;
    top: 5px;
    left: 5px;
}
#mapPane {
    height: 100%;
    padding: 0;
}
#map {
    height: 100%;
}

.nonModal_underlay {
